{
  "name": "@ranger/triggers",
  "version": "1.0.0",
  "description": "Extensible trigger system for Ranger agents - Schedule, Webhook, Events",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c --bundleConfigAsCjs",
    "clean": "rimraf dist",
    "dev": "rollup -c --bundleConfigAsCjs --watch",
    "lint": "eslint --ext .ts src/",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.5",
    "@types/node-cron": "^3.0.11",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "triggers",
    "scheduler",
    "cron",
    "webhook",
    "events",
    "agents",
    "automation",
    "ranger"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
